<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimaryDark</item>
        <item name="colorPrimaryDark">@color/colorPrimary</item>
        <item name="colorAccent">@android:color/holo_blue_bright</item>
    </style>

    <style name="AppThemeStart" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/com_facebook_messenger_blue</item>
        <item name="colorPrimaryDark">@color/startColor</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>

    <style name="AppThemeQp" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorAc</item>
        <item name="colorPrimaryDark">@color/actionbar</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>

    <style name="AppThemesyll" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/primarSyll</item>
        <item name="colorPrimaryDark">@color/actionbarsy</item>
        <item name="colorAccent">?attr/colorPrimary</item>

    </style>

    <style name="AppThemeAbout" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/EPvoilet</item>
        <item name="colorPrimaryDark">@color/EPvoilet</item>
        <item name="colorAccent">?attr/colorPrimary</item>

    </style>

    <style name="AppThemeBook" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/bookPrim</item>
        <item name="colorPrimaryDark">@color/bookDark</item>
        <item name="colorAccent">?attr/colorPrimary</item>
        <item name="android:navigationBarColor">@color/bookPrim</item>

    </style>


</resources>
